generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TelegramBotUser {
  id Int @id

  username      String?
  language_code String?

  first_name String
  last_name  String?

  links Link[]
}

model Link {
  id String @id @default(cuid())

  name String
  url  String

  user   TelegramBotUser @relation(fields: [userId], references: [id])
  userId Int
}
